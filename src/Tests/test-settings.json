    "Consumers": {
      "simple_string_string": {
        "Topic": "simple_string_string-${env}",
        "BufferSize": 5000,
        "WaiterSettings": {
          "RetryCount": 10,
          "Interval": "00:00:01"
        },
        "SubscribeOverAssign": false,
        "IndependentConfig": true,
        "Config": {
          "bootstrap.servers": "localhost:9092",
          "group.id": "static_consumer_group_id37",
