{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "KafkaSettings": {
    "TemplateVariables": {
      "env": "local"
    },
    "DefaultClientConfig": {
      "bootstrap.servers": "localhost:9092",
